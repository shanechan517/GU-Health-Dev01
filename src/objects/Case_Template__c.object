    <validationRules>
        <fullName>Fixed_SLA_Days</fullName>
        <active>true</active>
        <description>To prevent non fixed sla&apos;s from not having the required criteria to calculate the SLAs</description>
        <errorConditionFormula>IF(
AND(
Fixed_SLA__c = false,
Code__c &gt; 0,
OR(
ISNULL(SLA2__c),
ISNULL(Organisation_Size__c)
)),true,false)</errorConditionFormula>
        <errorMessage>If the SLA days are not fixed, the SLA Days 2 field &amp; the Organisation Size fields are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_Case_Code</fullName>
        <active>true</active>
        <description>Ensure that all parent cases have code 0</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;Parent&apos;, Code__c &gt; 0 )</errorConditionFormula>
        <errorMessage>A Parent Case must have a code of 0.</errorMessage>
    </validationRules>

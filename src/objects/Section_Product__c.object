    <validationRules>
        <fullName>Ambulance_is_mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Ambulance__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Ambulance__c</errorDisplayField>

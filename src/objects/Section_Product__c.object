    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
OR(
AND(
NOT(ISBLANK(Hospital_Date_Withdrawn__c)),
Hospital_Date_Withdrawn__c &lt;= TODAY()),
AND(
NOT(ISBLANK(Medical_Date_Withdrawn__c)),
Medical_Date_Withdrawn__c &lt;= TODAY()),
AND(
NOT(ISBLANK(Ancillary_Date_Withdrawn__c)),
Ancillary_Date_Withdrawn__c &lt;= TODAY()))
,&apos;Component Withdrawn&apos;,&apos;Active&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>

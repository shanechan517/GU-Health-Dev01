    </validationRules>
    <validationRules>
        <fullName>Overseas_Visitor_Validation</fullName>
        <active>false</active>
        <description>Mandatory when Resident status = Overseas Visitor</description>
        <errorConditionFormula>AND(
INCLUDES(Resident_status__c, &apos;Overseas Visitor&apos;),
ISNULL(Overseas_Visitor_status__c)
)</errorConditionFormula>
        <errorMessage>Overseas Visitor Status is mandatory when Resident Status equals &apos;Overseas Visitor&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>When_Product_type_Chng_on_Cover_Code_set</fullName>
        <active>true</active>
        <errorConditionFormula>Cover_Code__c != null &amp;&amp;   ISCHANGED(Product_Type__c)</errorConditionFormula>
        <errorDisplayField>Product_Type__c</errorDisplayField>
        <errorMessage>Cannot change Product Type when Cover Code is set. To change Product Type, clear the value for Cover Code and Save changes with new Product and then re-apply Cover Code as applicable</errorMessage>
    </validationRules>

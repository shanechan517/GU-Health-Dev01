    <validationRules>
        <fullName>Close_SPL_in_Draft_Only</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
AND( 
ISCHANGED(Date_closed__c), 
Section__r.Group__r.RecordType.Name != &apos;Draft&apos;), 
TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Date_closed__c</errorDisplayField>
        <errorMessage>You can only close a section product link via a draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Date_cannot_be_before_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>if( Start_Date__c  &gt;   Date_closed__c  ,true,false)</errorConditionFormula>
        <errorMessage>Closed Date cannot be before Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_is_required_if_Date_closed</fullName>
        <active>true</active>
        <errorConditionFormula>if( ISNULL(Start_Date__c ) &amp;&amp;  !ISNULL(Date_closed__c) ,true,false)</errorConditionFormula>
        <errorMessage>Start date is required if Date Closed is populated</errorMessage>
    </validationRules>

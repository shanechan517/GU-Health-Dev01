        <externalId>false</externalId>
        <formula>if(ISPICKVAL(Type__c, &apos;H&apos;) ,&apos;Hospital&apos;,
     if(ISPICKVAL(Type__c, &apos;A&apos;) ,&apos;Ancillary&apos;,
